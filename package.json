  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "next": "8.0.3",
    "next-routes": "1.4.2",
    "prop-types": "^15.6.2",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-redux": "^6.0.0",
    "react-router": "4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "babel-loader": "8.0.4",
    "babel-plugin-lodash": "3.3.4",
