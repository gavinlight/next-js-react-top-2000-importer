    "lint": "eslint src/",
    "analyze:build": "BUNDLE_ANALYZE=both npm run build",
    "analyze:build:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:build:browser": "BUNDLE_ANALYZE=browser npm run build"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel",
    "styled-components"
