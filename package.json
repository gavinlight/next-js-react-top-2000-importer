    "eslint-import-resolver-babel-module": "^5.0.0-beta.1",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "json-loader": "0.5.7",
    "rimraf": "^2.6.2",
    "svg-react-loader": "0.4.6",
    "url-loader": "1.1.2",
    "webpack": "4.25.1"
  },
  "engines": {
    "node": "8.9.1"
  }
