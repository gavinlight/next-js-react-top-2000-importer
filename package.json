{
  "name": "next-js-react-top-2000-importer",
  "version": "0.0.1",
  "description": "Import your own top 2000 list directly into a spotify playlist",
  "author": "gavinlight",
  "repository": {
    "type": "git",
    "url": "https://github.com/gavinlight/next-js-react-top-2000-importer"
  },
  "license": "MIT",
  "scripts": {
    "start": "node server/index.js",
    "build": "npm run clean && next build src",
    "server": "NODE_ENV=production node server/index.js",
